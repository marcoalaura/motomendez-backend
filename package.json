{
  "name": "centralizador_pcd",
  "version": "0.0.1",
  "description": "Sistema de centralizado de personas con discapacidad",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "generar-tests": "babel-node scripts/generar-tests.js",
    "test": "NODE_ENV=test FORCE=true babel-node index.js && sequelize db:seed:all --env test --config src/config/config.json --seeders-path src/seeders/ && sequelize db:migrate:undo:all --env test --config src/config/config.json --migrations-path src/migrations && sequelize db:migrate --env test --config src/config/config.json --migrations-path src/migrations && NODE_ENV=test mocha --timeout 25000 test/**/**/*.js",
    "lint": "eslint *.js test/*.js eslint src --ext js",
    "apidoc": "apidoc -f '.*\\.route.js' -o public/apidoc",
    "apidoc-servicio": "apidoc -f '.servicio.route.js' -o public/apidoc-servicio",
    "setup": "FORCE=true babel-node index.js && sequelize db:seed:all --config src/config/config.json --seeders-path src/seeders && sequelize db:migrate:undo:all --config src/config/config.json --migrations-path src/migrations && sequelize db:migrate --config src/config/config.json --migrations-path src/migrations",
    "instalar": "npm install",
    "startdev": "nodemon --exec 'babel-node index.js'",
    "build": "babel -d dist/src -D src && babel -d dist -D index.js && cp package.json dist && cp -R node_modules dist",
    "funciones": " sequelize db:migrate --config src/config/config.json --migrations-path src/migrations",
    "regularizar-files": "babel-node reg.js"
  },
  "keywords": [
    "node",
    "base",
    "sistema centralizador-pcd"
  ],
  "apidoc": {
    "name": "API REST - Documentacion de centralizador-pcd",
    "template": {
      "forceLanguage": "es"
    }
  },
  "author": "jupa1986",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.17.5",
    "async": "^2.0.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.25.0",
    "babel-watch": "^2.0.7",
    "base-64": "^0.1.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "consign": "^0.1.2",
    "continuation-local-storage": "^3.2.0",
    "cors": "^2.7.1",
    "csvtojson": "^2.0.8",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-validation": "^1.0.2",
    "formidable": "^1.2.1",
    "fs-extra": "^2.0.0",
    "handlebars": "^4.0.11",
    "hashids": "^1.1.4",
    "helmet": "^3.4.0",
    "html-pdf": "^2.2.0",
    "intl": "^1.2.5",
    "intl-messageformat": "^1.3.0",
    "joi": "^10.6.0",
    "json2csv": "^4.1.3",
    "jsreport": "^1.7.0",
    "jsreport-core": "^1.3.2",
    "jwt-simple": "^0.5.0",
    "konfig": "^0.2.1",
    "moment": "^2.19.2",
    "node-rest-client": "^1.8.0",
    "nodemailer": "^4.4.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-ldapauth": "^1.0.0",
    "pg": "^4.5.2",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.13",
    "qr-image": "^3.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.2",
    "sequelize": "^3.21.0",
    "sequelize-formly": "^1.0.19",
    "sequelize-handlers": "^0.1.7",
    "underscore": "^1.8.3",
    "utf8": "^2.1.1",
    "uuid": "^3.1.0",
    "winston": "^2.2.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^2.4.5",
    "should": "^9.0.2",
    "supertest": "^1.2.0",
    "nodemon": "^1.19.1"
  }
}
